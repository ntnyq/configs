{
  "name": "@ntnyq/configs",
  "version": "1.3.1",
  "private": true,
  "packageManager": "pnpm@7.27.0",
  "scripts": {
    "lint": "eslint --cache .",
    "prepare": "husky install",
    "release": "run-s release:check release:version release:publish",
    "release:check": "run-s lint test",
    "release:publish": "pnpm -r publish --access public",
    "release:version": "bumpp package.json packages/*/package.json",
    "test": "vitest"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "workspace:*",
    "@types/eslint": "^8.21.0",
    "@types/node": "^18.13.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.34.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.27.0",
    "vitest": "^0.28.4"
  },
  "nano-staged": {
    "*.{js,ts,json,md,yml,yaml}": "eslint --fix"
  }
}
