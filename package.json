{
  "name": "@ntnyq/configs",
  "type": "module",
  "version": "2.0.0",
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "lint": "eslint",
    "prepare": "husky",
    "release": "run-s release:check release:version release:publish",
    "release:check": "run-s lint test",
    "release:publish": "pnpm -r publish --access public",
    "release:version": "bumpp -r",
    "test": "pnpm -r run test"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^5.1.0",
    "@ntnyq/prettier-config": "^3.0.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.16.5",
    "bumpp": "^10.2.0",
    "eslint": "^9.31.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^8.0.4",
    "pnpm": "^10.13.1",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,md,yml,yaml,json}": "eslint --fix"
  }
}
