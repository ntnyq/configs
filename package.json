{
  "name": "@ntnyq/configs",
  "type": "module",
  "version": "2.0.0",
  "private": true,
  "packageManager": "pnpm@10.4.1",
  "scripts": {
    "lint": "eslint .",
    "prepare": "husky",
    "release": "run-s release:check release:version release:publish",
    "release:check": "run-s lint test",
    "release:publish": "pnpm -r publish --access public",
    "release:version": "bumpp -r",
    "test": "pnpm -r run test"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^4.0.0-beta.6",
    "@ntnyq/prettier-config": "workspace:*",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.13.4",
    "bumpp": "^10.0.3",
    "eslint": "^9.20.1",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "pnpm": "^10.4.1",
    "prettier": "^3.5.1",
    "typescript": "^5.7.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "protobufjs"
    ]
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,json,md,yml,yaml}": "eslint --fix"
  }
}
