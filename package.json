{
  "name": "@ntnyq/configs",
  "version": "1.0.2",
  "private": true,
  "packageManager": "pnpm@7.1.0",
  "scripts": {
    "lint": "eslint .",
    "prepare": "husky install",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "workspace:*",
    "bumpp": "^7.1.1",
    "eslint": "^8.17.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "pnpm": "^7.1.9"
  },
  "lint-staged": {
    "*.{js,ts,json,md,yaml}": "eslint --fix"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@ntnyq/eslint-config"
  }
}
