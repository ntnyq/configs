{
  "name": "@ntnyq/configs",
  "type": "module",
  "version": "3.1.0",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "lint": "eslint",
    "prepare": "husky",
    "release": "run-s release:check release:version",
    "release:check": "run-s lint test",
    "release:version": "bumpp -r",
    "test": "pnpm -r run test"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^5.9.0",
    "@ntnyq/prettier-config": "^3.0.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.10.9",
    "bumpp": "^10.4.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "nano-staged": "^0.9.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3"
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,md,yml,yaml,json}": "eslint --fix"
  }
}
